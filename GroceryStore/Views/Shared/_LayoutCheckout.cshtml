                }

                    //var parentTable = $(this).parent().parent().parent().parent();
                    //var productDivData = $(productDiv).data("cartBoxDivData");

                else {
                    $(this).css("border-color", "");
                    qtyValue += parseFloat(productQtyText);
                    var unitPriceText = $(this).parent().next("td.unitPrice").text();
                    var unitPriceValue = parseFloat(unitPriceText);

                    var productPrice = qtyValue * unitPriceValue;
                    var productPriceRoundedValue = productPrice.toFixed(2);

                    //$(productDiv).find("span.cartproductprice").text("Rs. " + productPriceRoundedValue).append(' <a class="cartproductRemove" href="javascript:void();">Remove</a>');
                    $(this).parent().siblings("td.tdTotalPrice").text(productPriceRoundedValue);

                    CheckOut.Structure.calculateCustomerOrderTotal();

                }


            });


            $(document).on("cut copy paste", ".timetable_sub input.tdQuantity", function (e) {
                e.preventDefault();
            });


            //$(".deliveryDate").datetimepicker({
                
            //    minDate: 0,
            //    format: 'Y-m-d H:i:s'
            //});
            
            //only lower uppercase alphabets, space is also allowed
            $("#txtFullName").on("keyup blur", function () {
                $(this).val(function (i, val) {
                    return val.replace(/[^a-z\s]/gi, '');
                });
            });


            $(document).on("click", ".check_out", function (e) {
                e.preventDefault();
                //alert("check out button is clicked");
                CheckOut.Structure.saveCustomerOrder();
            });


            //for cart items       quantity  text box
            //This will let you enter positive numbers only and the decimal numbers with 2 digits precision
            $(document).on("keypress", 'table.timetable_sub input.tdQuantity', function (event) {
                //alert("key is pressed");
                //if ((event.which != 46 || $(this).val().indexOf('.') != -1) && (event.which < 49 || event.which > 57)) {
                //    if (event.keyCode !== 8 && event.keyCode !== 46) { //exception
                //        event.preventDefault();
                //    }
                //}
                //if (($(this).val().indexOf('.') != -1) && ($(this).val().substring($(this).val().indexOf('.'), $(this).val().indexOf('.').length).length > 2)) {
                //    if (event.keyCode !== 8 && event.keyCode !== 46) { //exception
                //        event.preventDefault();
                //    }
                //}


                //this will allow numbers from 0 to 9    without decimal
                $(this).val($(this).val().replace(/[^\d].+/, ""));
                //$(this).val($(this).val().replace(/[^1-9\.]/g, ''));
                if ((event.which < 48 || event.which > 57)) {
                    event.preventDefault();
                }

            });


        });
    </script>


